{
    "name": "lsv/food-market-integration-wrapper",
    "description": "Wrapper for sinqro food market integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Aarhof",
            "email": "martin.aarhof@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/options-resolver": "^5.4 | ^6.0",
        "symfony/serializer": "^5.4 | ^6.0",
        "symfony/property-access": "^5.4 | ^6.0",
        "psr/http-client": "^1.0",
        "nyholm/psr7": "^1.4",
        "symfony/http-client": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Lsv\\FoodMarketIntegration\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "php-http/mock-client": "^1.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Lsv\\FoodMarketIntegrationTest\\": "tests"
        }
    },
    "scripts": {
        "fix": [
            ".test/fix.sh"
        ]
    }
}
